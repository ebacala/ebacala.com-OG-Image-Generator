{
  "name": "ebacala.com - OG Image Generator",
  "version": "1.0.0",
  "description": "Simple tool to generate the OG images for ebacala.com",
  "main": "./dist/index.js",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "node script.js",
    "build": "esbuild generateOgImage.js --bundle --platform=node --target=esnext --minify --packages=external --format=esm --outfile=dist/index.js"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "puppeteer": "^24.16.1"
  },
  "devDependencies": {
    "esbuild": "^0.25.8",
    "typescript": "^5.9.2"
  }
}
